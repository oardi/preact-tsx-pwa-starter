{
	"compileOnSave": false,
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"lib": [
			"es2015",
			"dom"
		],
		"module": "ESNext",
		"moduleResolution": "node",
		"sourceMap": true,
		"target": "es5",
		"resolveJsonModule": true,
		"esModuleInterop": true
	},
	"include": [
		"src/**/*"
	]
}
